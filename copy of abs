(1,n,0,0) #input no. n stored in mem[0]
(16,1,0,1) #count stored in mem[1]
(16,1,0,2) #result stored in mem[2]
(16,1,0,5) #1 stored in mem[5]
(12,1,0,3) #true if n < count stored in mem[3]
(13,3,0,9) #breakpoint of while loop
(8,2,1,2) #result = result*count
(6,1,5,1) #count = count +1
(14,0,0,4) #while loop again on
(15,2,0,0) #print output
(0,0,0,0) #stop